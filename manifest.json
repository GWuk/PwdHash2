{

 "manifest_version": 2,
 "name": "PwdHash",
 "version": "2.0",

 "description": "Automatically generates per-site passwords if you prefix your password with @@ or F2.",

 "applications": {
   "gecko": {
     "id": "{bb8d77b0-a845-4249-a205-ef7395587b69}",
     "strict_min_version": "42.0"
   }
 },
 "author": "Stanford University",
 "homepage_url": "http://crypto.stanford.edu/PwdHash",

 "icons": {
   "64": "icon64.png",
   "96": "icon.svg"
 },

 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["chrome/content/domain-extractor.js","chrome/content/hashed-password.js","chrome/content/stanford-pwdhash.js","chrome/content/md5.js","chrome/content/forge.min.js"]
   }
 ],
 
 "options_ui": {
   "page": "chrome/content/options.html"
 },

 "permissions": ["storage"]

}
